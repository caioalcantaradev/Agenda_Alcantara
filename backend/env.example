# MongoDB Atlas Connection String
# Obtenha em: https://cloud.mongodb.com/
# Formato: mongodb+srv://usuario:senha@cluster.mongodb.net/agenda
MONGODB_URI=mongodb+srv://usuario:senha@cluster.mongodb.net/agenda

# Secret para JWT (use uma string aleatória e segura)
JWT_SECRET=seu-secret-jwt-aqui

# Porta do servidor (opcional, padrão: 5000)
PORT=5000

# CORS Origin (URL do frontend)
CORS_ORIGIN=http://localhost:3000

